<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Layer 3: Outer Circle -->
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/brown" /> <!-- Replace with your desired color -->
        </shape>
    </item>

    <!-- Layer 2: Middle Circle -->
    <item android:top="7dp" android:left="7dp" android:right="7dp" android:bottom="7dp">
        <shape android:shape="oval">
            <solid android:color="@color/white" /> <!-- Replace with your desired color -->
        </shape>
    </item>

    <!-- Layer 1: Inner Circle -->
    <item android:top="10dp" android:left="10dp" android:right="10dp" android:bottom="10dp">
        <shape android:shape="oval">
            <solid android:color="@color/brown" /> <!-- Replace with your desired color -->
        </shape>
    </item>

</layer-list>